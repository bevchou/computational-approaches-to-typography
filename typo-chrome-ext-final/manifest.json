{
  "name": "typography magic",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Comp Typo Final",
  "browser_action": {
  },

  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["jquery.js"]
  }],

  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "activeTab",
    "<all_urls>"
  ]
}
